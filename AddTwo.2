; AddTwo.asm - adds two 32-bit integers.
; Chapter 3 example

.386
.model flat,stdcall
.stack 4096
ExitProcess proto,dwExitCode:dword
.data
v_result, dword ?
v_b, dword 98h
v_c, dword 2h
v_d, dword 98h

.code
main proc
mov eax 0
add eax, b
add eax, c
sub eax, d
move v_result, eax

	invoke ExitProcess,0
main endp
end main